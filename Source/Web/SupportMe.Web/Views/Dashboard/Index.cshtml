@model dynamic

@{
    ViewBag.Title = "Dashboard";
}
<br />
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <h2 class="well text-center">Dashboard</h2>
        </div>
    </div>
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-body">
                @Html.Action("TicketStatePie", "Statistics", new { area = string.Empty })
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title text-center">Tickets</h3>
            </div>
            <div class="panel-body">
                @Html.Action("GetAllInGrid", "Ticket", new { area = string.Empty })
            </div>
        </div>
    </div>
</div>
